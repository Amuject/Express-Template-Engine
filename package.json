{
  "name": "express-template-engine",
  "version": "0.2.0",
  "description": "Easy HTML-based template engine for Express",
  "author": "Wany <sung@wany.io> (https://wany.io)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist-cjs/index.js"
    }
  },
  "module": "./dist-esm/index.js",
  "main": "./dist-cjs/index.js",
  "keywords": [
    "html",
    "template",
    "express",
    "template-engine"
  ],
  "homepage": "https://github.com/wnynya/node-Express-Template-Engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/wnynya/node-Express-Template-Engine.git"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs && npm run build:package",
    "build:clean": "node scripts/build-clean.js",
    "build:esm": "tsc --p tsconfig.json",
    "build:cjs": "npx babel dist-esm --out-dir dist-cjs",
    "build:package": "node scripts/build-package.js",
    "test": "npm run test:clean && npm run test:esm && npm run test:cjs",
    "test:clean": "node scripts/test-clean.js && npm install",
    "test:esm": "npm run -w ./test test:esm",
    "test:cjs": "npm run -w ./test test:cjs",
    "bnt": "npm run build && npm run test"
  },
  "workspaces": [
    "test"
  ],
  "dependencies": {
    "jsdom": "^24.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.3.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
